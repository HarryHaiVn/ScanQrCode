<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.MainActivity2">

    <include
        android:id="@+id/toolbar"
        layout="@layout/toolbar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:cardCornerRadius="5dp"
        android:elevation="15dp">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="10dp"
            android:paddingBottom="10dp">
            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/fab"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/d_scan_image_white"
                app:backgroundTint="@color/colorRed"
                app:elevation="10dp"
                app:fabCustomSize="80dp"
                app:maxImageSize="32dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toTopOf="@+id/textView"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/fab1"/>
            <TextView
                android:id="@+id/textView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/quicksand_medium"
                android:text="@string/scan_image"
                app:layout_constraintTop_toBottomOf="@+id/fab"
                app:layout_constraintStart_toStartOf="@+id/fab"
                app:layout_constraintEnd_toEndOf="@+id/fab"
                app:layout_constraintBottom_toTopOf="@+id/fab3" />
            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/fab1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/d_scan_white"
                app:backgroundTint="@color/colorYellow"
                app:elevation="10dp"
                app:fabCustomSize="80dp"
                app:maxImageSize="32dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toEndOf="@+id/fab"
                app:layout_constraintEnd_toStartOf="@+id/fab2" />
            <TextView
                android:id="@+id/textView1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/quicksand_medium"
                android:text="@string/scan"
                app:layout_constraintTop_toBottomOf="@+id/fab1"
                app:layout_constraintStart_toStartOf="@+id/fab1"
                app:layout_constraintBottom_toBottomOf="@+id/textView"
                app:layout_constraintEnd_toEndOf="@+id/fab1" />
            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/fab2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/d_history_white"
                app:backgroundTint="@color/color"
                app:elevation="10dp"
                app:fabCustomSize="80dp"
                app:maxImageSize="32dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toEndOf="@+id/fab1"
                app:layout_constraintEnd_toEndOf="parent"/>
            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/quicksand_medium"
                android:text="@string/history"
                app:layout_constraintTop_toBottomOf="@+id/fab2"
                app:layout_constraintStart_toStartOf="@+id/fab2"
                app:layout_constraintBottom_toBottomOf="@+id/textView"
                app:layout_constraintEnd_toEndOf="@+id/fab2" />
            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/fab3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/d_favorites_white"
                app:backgroundTint="@color/color1"
                app:elevation="10dp"
                app:fabCustomSize="65dp"
                app:layout_constraintTop_toBottomOf="@+id/textView"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/fab4"
                app:layout_constraintBottom_toTopOf="@+id/textView3"/>
            <TextView
                android:id="@+id/textView3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/quicksand_medium"
                android:text="@string/favorite"
                app:layout_constraintTop_toBottomOf="@+id/fab3"
                app:layout_constraintStart_toStartOf="@+id/fab3"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/fab3" />
            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/fab4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/d_my_qr_white"
                app:backgroundTint="@color/color2"
                app:elevation="10dp"
                app:fabCustomSize="65dp"
                app:layout_constraintTop_toTopOf="@+id/fab3"
                app:layout_constraintStart_toEndOf="@+id/fab3"
                app:layout_constraintEnd_toStartOf="@+id/fab5"/>
            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/quicksand_medium"
                android:text="@string/create_my_qr"
                app:layout_constraintTop_toTopOf="@+id/textView3"
                app:layout_constraintStart_toStartOf="@+id/fab4"
                app:layout_constraintBottom_toBottomOf="@+id/textView3"
                app:layout_constraintEnd_toEndOf="@+id/fab4" />
            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/fab5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/d_create_white"
                app:backgroundTint="@color/color3"
                app:elevation="10dp"
                app:fabCustomSize="65dp"
                app:layout_constraintTop_toTopOf="@+id/fab3"
                app:layout_constraintStart_toEndOf="@+id/fab4"
                app:layout_constraintEnd_toStartOf="@+id/fab6" />
            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/create_qr"
                android:fontFamily="@font/quicksand_medium"
                app:layout_constraintTop_toTopOf="@+id/textView3"
                app:layout_constraintStart_toStartOf="@+id/fab5"
                app:layout_constraintBottom_toBottomOf="@+id/textView3"
                app:layout_constraintEnd_toEndOf="@+id/fab5" />
            <com.google.android.material.floatingactionbutton.FloatingActionButton
                android:id="@+id/fab6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/d_settings_white"
                app:backgroundTint="@color/color4"
                app:elevation="10dp"
                app:fabCustomSize="65dp"
                app:layout_constraintTop_toTopOf="@+id/fab3"
                app:layout_constraintStart_toEndOf="@+id/fab5"
                app:layout_constraintEnd_toEndOf="parent"/>
            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/quicksand_medium"
                android:text="@string/setting"
                app:layout_constraintTop_toTopOf="@+id/textView3"
                app:layout_constraintStart_toStartOf="@+id/fab6"
                app:layout_constraintBottom_toBottomOf="@+id/textView3"
                app:layout_constraintEnd_toEndOf="@+id/fab6" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>